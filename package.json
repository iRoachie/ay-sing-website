{
  "name": "ay-sing-website",
  "version": "1.0.0",
  "author": "Kyle Roach <kroach.work@gmail.com>",
  "dependencies": {
    "gatsby": "^2.0.19",
    "gatsby-image": "^2.0.15",
    "gatsby-plugin-google-analytics": "^2.0.7",
    "gatsby-plugin-manifest": "^2.0.5",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-source-filesystem": "^2.0.4",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-with-async-fonts": "^4.0.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build:css": "tailwind build ./src/index.tailwind.css -c ./tailwind.js -o ./src/index.css",
    "build": "npm run build:css && purgecss -c ./purgecss.config.js -o ./src && gatsby build",
    "develop": "npm run build:css && gatsby develop",
    "start": "npm run develop",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "prettier": "^1.14.2",
    "purgecss": "^1.1.0",
    "tailwindcss": "^0.7.0"
  },
  "repository": "git@gitlab.com:ay-sing/ay-sing-website.git"
}
